{
  "expo": {
    "name": "Fittbot Business",
    "slug": "fittbot-business",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "adminapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.fittbot.fittbot-business",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSCameraUsageDescription": "This app needs access to the camera to take images for community feed functionality.",
        "NSPhotoLibraryUsageDescription": "This app needs access to the photo library to use images for community feed functionality."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.fittbot.fittbot_business",
      "softwareKeyboardLayoutMode": "pan",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/notification-icon.png",
          "color": "#FF5757",
          "androidMode": "default",
          "androidCollapsedTitle": "Fittbot Business",
          "androidSmallIcon": "./assets/images/notification-icon.png"
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow this app to access your photos to manage gym feed.",
          "cameraPermission": "Allow this app to access your camera to take workout progress photos for gym feed."
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ],
      [
        "expo-location",
        {
          "locationWhenInUsePermission": "This app needs access to your location only when checking in at the gym."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "ba5670ac-efd7-4f6b-a538-2db185d42d8f"
      },
      "backendUrl": "https://app.fittbot.com",
      "backendPort": "8000",
      "disableHMRWarning": true
    },

    "owner": "martinraju"
  }
}
